@{ 
	string Title = Model.WithoutSettings.String(BlogKeys.Title);
	string Description = "Michael Burch is a technologist, cloud enthusiast, programmer, runner, hiker, and more.";
	string Excerpt =  (string.IsNullOrWhiteSpace(Model.WithoutSettings.String(BlogKeys.Excerpt)) ? Description : Model.WithoutSettings.String(BlogKeys.Excerpt));
	string PageTitle =  (string.IsNullOrWhiteSpace(Title) ? Context.String(BlogKeys.Intro) : Title);
	string ImageUrl = (string.IsNullOrWhiteSpace(Model.WithoutSettings.String(BlogKeys.Image)) ? Context.GetLink("/images/ogcard.jpeg", true) : Context.GetLink(Model.WithoutSettings.String(BlogKeys.Image),true));
	string Author = Model.String("Author");
	var Tags = Model.Get<string[]>(BlogKeys.Tags);
	var KeywordList = new List<string>()
	{
		"Michael Burch",
		"Michael",
		"Burch",
		"Azure", 
		"Cloud",
		"dotnet",
		".NET",
		"C#",
		"Python",
		"michaelburch",
		"Round Rock",
		"Texas",
		"Austin",
		"ATX"
	};
	if (Tags != null)
    {
		foreach (string tag in Tags.OrderBy(x => x))
        {
            KeywordList.Add(tag);
        }
    }
	var Keywords = string.Join(",",KeywordList);
}
<meta charset="utf-8" />
<meta http-equiv="Content-Language" content="en">
		<meta name="viewport" content="width=device-width, initial-scale=1" />
		<link rel="stylesheet" href="@Context.GetLink("assets/css/main.css")" />
		<link rel="stylesheet" href="@Context.GetLink("assets/css/xcode.min.css")" >
		<link rel="stylesheet" href="@Context.GetLink("assets/css/vs2015.min.css")" media='(prefers-color-scheme: dark)'>
		<link rel="preload" href="@Context.GetLink("assets/css/cookieconsent.min.css")" as="style" onload="this.onload=null;this.rel='stylesheet'">
		<meta name="description" content="@Description">
@if (Context.String(BlogKeys.RssPath) != null) {
<link type="application/rss+xml" rel="alternate" title="@Context.String(BlogKeys.Title)"
    href="@Context.GetLink(Context.String(BlogKeys.RssPath))" />
}
@if (Context.String(BlogKeys.AtomPath) != null) {
<link type="application/atom+xml" rel="alternate" title="@Context.String(BlogKeys.Title)"
    href="@Context.GetLink(Context.String(BlogKeys.AtomPath))" />
}
