@{
string Title = Model.WithoutSettings.String(BlogKeys.Title);
string PageTitle = Context.String(BlogKeys.Title) + (string.IsNullOrWhiteSpace(Title) ? string.Empty : " - " + Title);
}
<!DOCTYPE html>
<html lang="en">

<head>
        @Html.Partial("_Head")
</head>

<body class="is-preload">
        <div id="wrapper">
                @Html.Partial("_Header")
                @Html.Partial("_Menu")
                <div id="main">
                        @RenderBody()
                </div>

                @Html.Partial("_Sidebar")
                <script src="https://cdn.commento.io/js/count.js" async></script>
                <script src=@Context.GetLink("assets/js/cookieconsent.min.js")></script>
        </div>
        @Html.Partial("_Footer")
        
        </body>
</html>
