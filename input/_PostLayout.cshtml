

@{
string Title = Model.WithoutSettings.String(BlogKeys.Title);
string PageTitle = Context.String(BlogKeys.Title) + (string.IsNullOrWhiteSpace(Title) ? string.Empty : " - " + Title);
string Slug = Model.String(Keys.RelativeFilePath).ToLower().Substring(Model.String(Keys.RelativeFilePath).LastIndexOf('/') +1).Replace(".html","");
string SourceFile = Model.String(Keys.SourceFileName);
string lead = Model.String(BlogKeys.Lead);
string imgUrl = Model.String(BlogKeys.Image);
}
<!DOCTYPE html>
<html>

<head>
        @Html.Partial("_Head")
</head>

<body class="is-preload">
        <div id="wrapper">
                @Html.Partial("_Header")
                
                <div id="main">
                        	<article class="post">
								<header>
									<div class="title">
										<h2>@Title</h2>
										@if(!string.IsNullOrWhiteSpace(lead))
                                        {
                                            <p>@lead</p>
                                        }
									</div>
									<div class="meta">
										<time class="published" datetime='@(Model.Get<DateTime>(BlogKeys.Published))'>@(Model.Get<DateTime>(BlogKeys.Published).ToString("MMMM dd, yyyy"))</time>
									</div>
								</header>
								<span class="image featured">
									@if(!string.IsNullOrWhiteSpace(imgUrl))
                                        {
											<img src="@imgUrl" alt="" />
										}
									</span>
								@RenderBody()
								@foreach(var key in Context.Keys)
    { key.ToString(); }
								<footer>
									<ul class="stats">
										<li><a href="#">General</a></li>
										<li><a href="#" class="icon solid fa-comment">128</a></li>
									</ul>
								</footer>
							</article>
                </div>

        <script src=@Context.GetLink("assets/js/jquery.min.js")></script>
		<script src=@Context.GetLink("assets/js/browser.min.js")></script>
		<script src=@Context.GetLink("assets/js/breakpoints.min.js")></script>
		<script src=@Context.GetLink("assets/js/util.js")></script>
		<script src=@Context.GetLink("assets/js/main.js")></script>
        <script defer src=@Context.GetLink("assets/js/cookieconsent.min.js")></script>
        </div>
        @Html.Partial("_Footer")
        </body>
        
</html>



